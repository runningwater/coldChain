/* pages/index/jobLog/jobLog.wxss */
@import '../../../utils/icon.wxss';
.time{
  position: relative;
  display:inline-block;
  width:245rpx; 
  border: 1px solid #ddd; 
  border-radius: 10rpx;
  padding: 10rpx 20rpx;
  margin-right:22rpx;
}
.top,.bottom{
  overflow: hidden;
}
.bottom{
 font-size: 26rpx;
 color:#888;
 margin-bottom:25rpx;
}

.timePicker{
  display: inline-block;
  font-size: 34rpx;
  color: #888;
  width:245rpx;
}
.icon-zhankai{
  position:absolute;
  right:24rpx;
  top:20rpx;
  width:26rpx;
  height:13rpx;
  color:#888;
}
.search{
  font-size: 34rpx;
  color:#009be2;
}

.containerList{
  margin-top:50rpx;
  border-bottom: 1px solid #ddd;
  font-size:14px;
  color: #666;
}
.hospitalName{
  color:#333;
  font-size:34rpx;
  margin-bottom:25rpx;
}
.sampleNum{
  font-size: 28rpx;
  color:#666;
  margin-bottom:15rpx;
}
.outDate{
  float:right;
}
.yellow{
  color:#ee750a;
}
.green{
  color:#1ccaa3;
}

