@import '../../../utils/icon.wxss';
.time{
  position: relative;
  display:inline-block;
  width:245rpx; 
  border: 1px solid #ddd; 
  border-radius: 10rpx;
  padding: 10rpx 20rpx;
  margin-right:22rpx;
}
.top,.bottom{
  overflow: hidden;
}
.bottom{
 font-size: 12px;
}
.sampleNum{
  float: right;  
}
.timePicker{
  display: inline-block;
  font-size: 34rpx;
  color: #888;
  width:245rpx;
}
.icon-down{
  position:absolute;
  right:24rpx;
  top:20rpx;
  width:26rpx;
  height:13rpx;
  color:#888;
}
.search{
  font-size: 34rpx;
  color:#009be2;
}

.containerList{
  margin-top:50rpx;
  border-bottom: 1px solid #ddd;
  font-size:14px;
  color: #666;
}
