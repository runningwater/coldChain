@import '../../../../../../utils/icon.wxss';
.total{
  font-size: 17px;
  color:#333;
  background-color: #f3f3f3;
  padding:0.8rem 0.53rem;
}

.scanInput{
  position: relative;
  border: 1px solid #bebebe;
  border-radius: 5px;
  color:#888;
  margin:1.33rem 0.53rem;  
  padding: 7px 8px;
}
.icon-saoma{
  position: absolute;
  display: inline-block;
  font-size:1.387rem;
  line-height: 1rem;
  right:0.53rem;
  top:0.747rem;
}
.scanLists{
  margin:0 0.53rem;
}
.scanNumber{
  float: right;
  font-size:28rpx;
  color:#666;
}
.yellow{
  color:#ee750a;
}
.green{
  color:#1ccaa3;
}

/*按钮*/
.btn{
  margin:1.33rem 0.53rem 0;
  border-radius:5px;

}
.btn button{
  background: #009BE2;
  color:#fff;
  font-size: 16px;
  font-weight: bold;
}