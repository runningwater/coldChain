@import '../../../../../../../utils/icon.wxss';
.total{
  font-size: 17px;
  color:#333;
  background-color: #f3f3f3;
  padding:30rpx 20rpx;
}
.scanInput{
  position: relative;
  border: 1px solid #bebebe;
  border-radius: 5px;
  color:#888;
  margin:50rpx 20rpx;  
  padding: 7px 8px;
}
.icon-saoma{
  position: absolute;
  display: inline-block;
  font-size:52rpx;
  line-height: 37.5rpx;
  right:20rpx;
  top:22rpx;
}

.scanLists{
  margin:0 20rpx;
}
.scanNumber{
  float: right;
  font-size:28rpx;
  color:#666;
}
.yellow{
  color:#ee750a;
}
.green{
  color:#1ccaa3;
}
.icon1{
  top:16rpx;
}
.icon-xiangji{
  font-size: 52rpx;
  color:#009BE2;
}
.icon-xiangce {
  font-size: 52rpx;
  right: 9%;
  color:#1ccaa3;
}
/*按钮*/
.btnGroup{
  overflow: hidden;
  margin-top:50rpx;
  padding:0 100rpx;
}
button{
  background: #009BE2;
  color:#fff;
  font-size: 16px;
  font-weight: bold;
}
.left{
  float:left;
}
.right{
  position: absolute;
  bottom:20rpx;
  right:20rpx;
}
checkbox-group{
  height: 880rpx;
  overflow-y: scroll;

}
/* 项目录入 */
checkbox-group{
  padding:0 20rpx;
}
.item{
  width:700rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.icon-xiangmu1{
  font-size: 52rpx;
  right: 18%;
  color:#e7b710;
}
.btns{
  position: absolute;
  bottom:20rpx;
}
.submitItems,.complete{
  display: inline-block;
  width:250rpx; 
  margin:30rpx 60rpx 0; 
}
.wrap{
  padding:30rpx 20rpx;
  background-color: #f3f3f3;
  color:#333;
  font-size: 32rpx;
}
.wrap text{
  color:#888;
  font-size: 28rpx;
}
