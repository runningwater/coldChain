<view class="container"  catchtap='cancel'>
  <view class="scanInput">
    <input placeholder-style="font-size:12px" confirm-type="done" bindconfirm="scanBag" placeholder="请扫描条码或输入条码" value="{{code}}" id="code" />
    <view class="img" bindtap="scanCode">
      <image src="../../../images/u477.png"></image>
    </view>
  </view>
    <view wx:for="{{barCode}}" >
     
     
     
    </view>

  <block wx:for="{{barCode}}">
    <view class='hang'>
    <view class='{{l==item.bagId?"w801":"w800"}}' data-bagid="{{item.bagId}}"  bindtouchstart="mytouchstart" 
    bindtouchmove="mymove" bindtouchend='myend'>
     <text class="fa fa-medkit X2"></text> 
      <text class="codeL box">{{item.barCode}}</text>     
      <text class="codeL">{{item.deviceName}}</text>
       <text class="fR">共<text class="red">{{item.sampleCount}}</text>个标本</text>
    </view><view class='w20' wx:if="{{l==item.bagId?true:false}}" bindtap='delBag' data-bagid="{{item.bagId}}">删除</view>    
</view> 
  </block>

  <view class="btn">
    <button bindtap="packing">完成</button>
  </view>
</view>