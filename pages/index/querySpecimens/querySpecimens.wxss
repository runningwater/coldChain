@import '../../../utils/icon.wxss';
.scanInput{
  position: relative;
  border: 1px solid #bebebe;
  border-radius: 5px;
  color:#333;  
  padding: 7px 8px;
  margin-bottom:30rpx;
}
.icon-saoma{
  position: absolute;
  display: inline-block;
  font-size:52rpx;
  line-height: 37.5rpx;
  right:20rpx;
  top:22rpx;
}
.barcode{
  font-size: 32rpx;
  color:#666;
  padding:20rpx;
}

.codeList{
  border:1px solid #bebebe;
  border-radius: 10rpx;
  font-size:14px;
  overflow: hidden;
  color:#333;
}
.buttonWrap{
  overflow: hidden;
  padding-top: 50rpx;
  border-top:1px solid #bebebe;
}
.iconButton{
  float:left;
  line-height:80rpx;
  text-align: center;
  height:190rpx;
  margin:20rpx 30rpx 0;
  width:24.4%;
}
.iconButton .iconfont{
  font-size:136rpx;
}
.iconButton view{
  font-size:28rpx;
  color:#333;
  line-height:28rpx;

}
.icon-wenshidu{
  color:#009BE2;
}
.icon-guiji{
  color:#ef7faf;
}
.icon-shenqingdan{
  color:#26d1a3;
}
.icon-xiangmu{
  color:#e7b710;
}
.show{
  display: block
}
.hidden{
  display: none
}
