.scanInput{
  border: 1px solid #3f89ec;
  border-radius: 10px;
  height: 30px;
  line-height: 30px;
  padding: 7px 8px;
  margin-top:30px;
  margin-bottom:20px;
}
.img{
  display: inline-block;
}
.img image{
  display: inline-block;
  width:20px;
  height: 20px;
  position: relative;
  top:0; 
}
.barcode{
  text-align: center;
  font-size: 60rpx;
  font-weight: bold
}

.codeList{
  font-size:14px;
  overflow: hidden;
  color:#333;
  padding:10px 8px 5px;

}
.fffcolor{
  color: #fff;
  margin-top: 15rpx;
  background: #0099CC;
  width: 30%;
  margin: 0 1.6%;
  float: left;
}
.buttonWrap{
  overflow: hidden;
  margin-top: 50px;
}
.show{
  display: block
}
.hidden{
  display: none
}
