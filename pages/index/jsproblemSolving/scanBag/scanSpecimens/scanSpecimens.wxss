/* pages/index/jsproblemSolving/scanBag/scanSpecimens/scanSpecimens.wxss */
@import '../../../../../utils/icon.wxss';
.header{
  background-color: #f3f3f3;
  padding:0.8rem 0.53rem;
  font-size:0.907rem;
  color:#333;
}
.scanInput{
  position: relative;
  border: 1px solid #bebebe;
  border-radius: 5px;
  color:#888;
  margin:1.33rem 0.53rem;  
  padding: 7px 8px;
}
.icon-saoma{
  position: absolute;
  display: inline-block;
  font-size:1.387rem;
  line-height: 1rem;
  right:0.53rem;
  top:0.747rem;
}

.codeList{ 
  overflow: hidden;
  color:#333;
  margin:0 0.53rem 0.8rem;
  border-bottom:1px solid #bebebe; 
}
.codeL{
  float: left;
  padding-bottom:20rpx;
}
.codeR{
  float: right;
  position:relative;
}
.scanDate{
  font-size:26rpx;
  color:#888;
}
.yellow{
  color:#ee750a;
}
.green{
  color:#1ccaa3;
}
.overflow{
  overflow:hidden;
  font-size: 16px;
}

.center{
  text-align:center
}

.icon-xiangji{
  font-size: 52rpx;
  color:#009BE2;
}
.icon-photo {
  font-size: 52rpx;
  right: 30%;
  color:#1ccaa3;
}
.del{
  width: 40px;
  height: 40px;
}