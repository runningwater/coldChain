/* pages/index/jsproblemSolving/scanBag/scanSpecimens/scanSpecimens.wxss */
@import '../../../../../utils/icon.wxss';
.header{
  background-color: #f3f3f3;
  padding:30rpx 20rpx;
  font-size:34rpx;
  color:#333;
}
.scanInput{
  position: relative;
  border: 1px solid #bebebe;
  border-radius: 10rpx;
  color:#888;
  margin:50rpx 20rpx;  
  padding: 14rpx 16rpx;
}
.icon-saoma{
  position: absolute;
  display: inline-block;
  font-size:52rpx;
  line-height: 37.5rpx;
  right:20rpx;
  top:28rpx;
}

.codeList{ 
  overflow: hidden;
  color:#333;
  margin:0 20rpx 30rpx;
  border-bottom:1px solid #bebebe; 
}
.codeL{
  float: left;
  padding-bottom:20rpx;
}
.codeR{
  float: right;
  position:relative;
}
.scanDate{
  font-size:26rpx;
  color:#888;
}
.yellow{
  color:#ee750a;
}
.green{
  color:#1ccaa3;
}
.overflow{
  overflow:hidden;
  font-size: 32rpx;
}

.center{
  text-align:center
}

.icon-xiangji{
  font-size: 52rpx;
  color:#009BE2;
}
.icon-photo {
  font-size: 52rpx;
  right: 30%;
  color:#1ccaa3;
}
.del{
  width: 80rpx;
  height: 80rpx;
}